<html>
  <head>
    <%= include partials/head.ejs %>
  </head>
  <body>
    <%= include partials/navbar.ejs %>
    <div class="page-header text-center">
      <h1>Error has occurred</h1>
    </div>
    <div class="container">
      <h2><%= message %></h2>
      <% if (error.status && error.stack) { %>
        <h3>Status: <%= error.status %></h3>
        <h4>Stack Trace:</h4>
        <pre><%= error.stack %></pre>
      <% } %>
    </div>
    <%= include partials/scripts.ejs %>
  </body>
</html>
