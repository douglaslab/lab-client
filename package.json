{
  "name": "douglaslab-sample-api-client",
  "version": "1.0.0",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "start": "node --harmony ./bin/www",
    "test": "mocha --harmony tests",
    "cover": "node --harmony ./node_modules/istanbul-harmony/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "lint": "eslint .",
    "doc": "jsdoc -c ./jsdoc.json .",
    "build": "bower install && gulp",
    "dev": "set API_URL=http://localhost:3000&& bower install && gulp && node --harmony ./bin/www",
    "postinstall": "bower install && gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglaslab/lab-client.git"
  },
  "author": "Guy Vider",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/douglaslab/lab-client/issues"
  },
  "homepage": "https://github.com/douglaslab/lab-client#readme",
  "dependencies": {
    "body-parser": "~1.13.1",
    "bower": "^1.4.1",
    "compression": "^1.5.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.2",
    "express": "~4.13.0",
    "express-session": "^1.11.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-bower": "0.0.10",
    "gulp-debug": "^2.0.1",
    "gulp-filter": "^2.0.2",
    "gulp-if": "^1.2.5",
    "gulp-minify-css": "^1.2.0",
    "gulp-rimraf": "^0.1.1",
    "gulp-uglify": "^1.2.0",
    "main-bower-files": "^2.8.2",
    "moment": "^2.10.3",
    "morgan": "~1.6.0",
    "passport": "^0.2.2",
    "passport-local": "^1.0.0",
    "request": "^2.58.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "istanbul-harmony": "^0.3.16",
    "mocha": "^2.2.5",
    "should": "^7.0.1",
    "supertest": "^1.0.1"
  }
}
