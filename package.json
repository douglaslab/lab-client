  "bugs": {
    "url": "https://github.com/douglaslab/lab-client/issues"
  },
  "homepage": "https://github.com/douglaslab/lab-client#readme",
  "dependencies": {
    "async": "^1.4.2",
    "babel": "^5.8.21",
    "babelify": "^6.3.0",
    "body-parser": "~1.14.1",
    "bower": "^1.4.1",
    "browserify": "^11.2.0",
    "compression": "^1.5.2",
    "connect-flash": "^0.1.1",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "~1.4.0",
    "debug": "~2.2.0",
    "dropbox": "^0.10.3",
    "ejs": "^2.3.2",
    "express": "~4.13.3",
    "express-session": "^1.11.3",
