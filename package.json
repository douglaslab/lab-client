    "gulp-bower": "0.0.10",
    "gulp-debug": "^2.0.1",
    "gulp-eslint": "^1.0.0",
    "gulp-filter": "^3.0.0",
    "gulp-if": "^2.0.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-rimraf": "^0.2.0",
    "gulp-uglify": "^1.2.0",
    "lab-api-wrapper": "^1.0.6",
    "main-bower-files": "^2.9.0",
    "moment": "^2.10.6",
    "morgan": "~1.6.1",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "vinyl-source-stream": "^1.1.0",
